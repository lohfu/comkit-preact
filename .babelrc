{
  "env": {
    "node": {
      "presets": [ "es2015-node6", "es2016", "es2017", "stage-0" ],
      "plugins": [
        "add-module-exports",
        ["transform-react-jsx", { "pragma": "h" }],
        ["module-resolver", {
          "alias": {
            "easy-tz": "easy-tz/cjs",
            "lowline": "lodash",
            "mini-qs": "querystring",
            "preact": "jsx-node"
          }
        }]
      ]
    }
  }
}
